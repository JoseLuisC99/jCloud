doctype html
html
	head
		title jCloud
		meta(charset="utf-8")
		script(src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.2/jquery.min.js")
		link(href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.6/css/materialize.min.css", rel="stylesheet")
		link(href="http://localhost:8080/assets/css/style.css" rel="stylesheet")
		link(href="https://maxcdn.bootstrapcdn.com/font-awesome/4.6.1/css/font-awesome.min.css" rel="stylesheet")
		script(src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.6/js/materialize.min.js")
		script(src="http://localhost:8080/assets/js/active-nav.js")
	body
		main
			aside#Folders.nav-fixed
				.nav-fixed-title 
					.user-avatar
					h5.center-align!= user
					h6.center-align!= email
				.nav-fixed-content
					ul.collection
						a(href="/jcloud/myCloud/").collection-item Mi unidad
						- for (var i = 1; i < folders.length; i++)
							a(href="/jcloud/myCloud/#{folders[i]}").collection-item!= folders[i]
			header#Main_header.teal
				h1.no-h.eslogan-cloud jCloud
				.teal.lighten-2
					a(href="/jcloud/myCloud" id="index").white-text.nav-item Disco
					a(href="/jcloud/config" id="config").white-text.nav-item Configuracion
					a(href="/jcloud/developers" id="developers").white-text.nav-item Developers
			span(style="padding: 3px; display: block;").teal.center-align.white-text.lighten-3 Mi unidad / #{dir}
			block content